{
  "name": "react-styled-frame",
  "version": "2.0.0",
  "description": "React iframe that works well with styled-components.",
  "author": "Pitis",
  "license": "MIT",
  "repository": "pitis/react-styled-frame",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "build": "rollup --config",
    "start": "rollup -c -w",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm i && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "keywords": [
    "react",
    "iframe",
    "frame",
    "styled",
    "styled-components"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@types/jest": "^27.0.3",
    "@types/react-frame-component": "^4.1.3",
    "gh-pages": "^3.2.3",
    "react-frame-component": "^5.2.1",
    "react-scripts": "^5.0.0",
    "rollup": "^2.61.1",
    "styled-components": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  }
}
